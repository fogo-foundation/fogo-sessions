{
  "name": "@fogo/sessions-idls",
  "version": "0.0.1",
  "private": true,
  "description": "The IDLs for the on-chain programs involved in the Fogo Sessions protocol",
  "keywords": [
    "fogo",
    "sessions",
    "crypto",
    "wallet"
  ],
  "type": "module",
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/esm/index.d.ts",
      "default": "./dist/esm/index.js"
    },
    "require": {
      "types": "./dist/cjs/index.d.ts",
      "default": "./dist/cjs/index.js"
    }
  },
  "scripts": {
  "build:cjs": "tsc --project tsconfig.build.cjs.json --verbatimModuleSyntax false --module commonjs --outDir ./dist/cjs && echo '{\"type\":\"commonjs\"}' > dist/cjs/package.json",
  "build:esm": "tsc --project tsconfig.build.esm.json --outDir ./dist/esm && echo '{\"type\":\"module\"}' > dist/esm/package.json",
  "build:package-json": "transform-package-json ./package.json dist/package.json"
  },
  "devDependencies": {
    "@cprussin/transform-package-json": "catalog:",
    "@cprussin/tsconfig": "catalog:",
    "typescript": "catalog:"
  }
}
