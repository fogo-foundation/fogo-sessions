{
  "$schema": "https://turbo.build/schema.json",
  "extends": [
    "//"
  ],
  "tasks": {
    "build": {
      "env": [
        "DISABLE_ACCESSIBILITY_REPORTING",
        "GOOGLE_ANALYTICS_ID",
        "VERCEL_ENV"
      ]
    },
    "start:dev": {
      "with": [
        "@fogo/sessions-sdk-react#start:dev",
        "@fogo/component-library#start:dev"
      ]
    },
    "test:types": {
      "dependsOn": [
        "build"
      ]
    }
  }
}